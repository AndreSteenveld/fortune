{
  "name": "fortune",
  "description": "A library for managing data in Node.js and web browsers.",
  "version": "3.4.1",
  "license": "MIT",
  "author": {
    "email": "0x8890@airmail.cc",
    "url": "http://0x8890.com"
  },
  "homepage": "http://fortunejs.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:fortunejs/fortune.git"
  },
  "bugs": "https://github.com/fortunejs/fortune/issues",
  "scripts": {
    "coverage": "istanbul cover test",
    "deploy": "npm run build && ./website/deploy.sh",
    "build": "node website/build",
    "lint": "eslint lib",
    "postpublish": "npm run deploy && npm run tag",
    "tag": "git tag `npm v fortune version` && git push origin --tags",
    "test": "npm run lint && npm run test:server && npm run test:browser && npm run test:websocket",
    "test:server": "node test | tf-dot",
    "test:browser": "browserify test/browser.js | tape-run | tf-dot",
    "test:websocket": "node test/integration/net/ws_server.js & browserify test/integration/net/ws_client.js | tape-run | tf-dot",
    "test:website": "npm run build && http-server dist/web"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "busboy": "^0.2.13",
    "error-class": "^2.0.0",
    "event-lite": "^0.1.1",
    "msgpack-lite": "^0.1.17",
    "negotiator": "^0.6.0",
    "ws": "^1.0.1"
  },
  "optionalDependencies": {
    "bufferutil": "^1.2.1",
    "utf-8-validate": "^1.2.1"
  },
  "devDependencies": {
    "@tap-format/dot": "^0.2.0",
    "browserify": "^13.0.0",
    "chalk": "^1.1.1",
    "cssnano": "^3.5.2",
    "docchi": "^0.12.0",
    "eslint": "^2.5.1",
    "eslint-config-0x8890": "^1.0.2",
    "form-data": "^1.0.0-rc4",
    "highlight.js": "^9.2.0",
    "html-minifier": "^1.3.1",
    "http-server": "^0.9.0",
    "inflection": "^1.8.0",
    "istanbul": "^0.4.2",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "mustache": "^2.2.1",
    "normalize.css": "^4.0.0",
    "postcss": "^5.0.19",
    "postcss-cssnext": "^2.5.1",
    "postcss-import": "^8.0.2",
    "rimraf": "^2.5.2",
    "tapdance": "^4.1.1",
    "tape-run": "^2.1.3"
  },
  "files": [
    "lib/",
    "test/",
    "LICENSE"
  ],
  "main": "lib/index.js",
  "browser": "lib/browser.js",
  "eslintConfig": {
    "extends": "0x8890",
    "ecmaFeatures": {
      "arrowFunctions": false,
      "binaryLiterals": false,
      "blockBindings": false,
      "classes": false,
      "defaultParams": false,
      "destructuring": false,
      "experimentalObjectRestSpread": false,
      "forOf": false,
      "generators": false,
      "globalReturn": false,
      "jsx": false,
      "modules": false,
      "objectLiteralComputedProperties": false,
      "objectLiteralDuplicateProperties": false,
      "objectLiteralShorthandMethods": false,
      "objectLiteralShorthandProperties": false,
      "octalLiterals": false,
      "regexUFlag": false,
      "regexYFlag": false,
      "restParams": false,
      "spread": false,
      "superInFunctions": false,
      "templateStrings": false,
      "unicodeCodePointEscapes": false
    },
    "env": {
      "es6": false
    },
    "rules": {
      "block-scoped-var": 2,
      "func-names": 0,
      "no-var": 0,
      "no-warning-comments": 1,
      "one-var": 0,
      "strict": [
        2,
        "global"
      ]
    }
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "keywords": [
    "adapter",
    "api",
    "application",
    "data",
    "middleware",
    "serializer",
    "transform"
  ]
}
