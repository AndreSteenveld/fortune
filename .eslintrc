{
  "env": {
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "camelcase": 2,
    "consistent-return": 2,
    "curly": [2, "multi"],
    "dot-notation": 0,
    "eol-last": 2,
    "eqeqeq": 2,
    "max-len": [2, 79, 4],
    "new-cap": [2, {"capIsNew": false}],
    "no-eq-null": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-shadow": 0,
    "no-trailing-spaces": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-vars": 2,
    "no-var": 2,
    "quotes": [2, "single"],
    "semi": [2, "never"],
    "semi-spacing": 0,
    "space-before-blocks": [2, "always"],
    "space-before-function-parentheses": [2, "always"],
    "space-in-brackets": [2, "always", {
      "singleValue": false,
      "objectsInArrays": true,
      "arraysInArrays": true,
      "arraysInObjects": true,
      "objectsInObjects": true,
      "propertyName": false
    }],
    "space-return-throw-case": 2
  }
}
